{
  "name": "@meltwater/makenew-serverless-nodejs",
  "version": "0.0.0",
  "description": "Package skeleton for a Node.js Serverless project on AWS Lambda.",
  "main": "dist/index.js",
  "module": "index.js",
  "sideEffects": false,
  "keywords": [
    "node"
  ],
  "homepage": "https://github.com/meltwater/makenew-serverless-nodejs",
  "bugs": "https://github.com/meltwater/makenew-serverless-nodejs/issues",
  "repository": "meltwater/makenew-serverless-nodejs",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": {
    "name": "Evan Sosenko",
    "email": "evan.sosenko@meltwater.com"
  },
  "files": [
    "yarn.lock",
    "index.js",
    "lib",
    "handlers",
    "data",
    "dist",
    "docs"
  ],
  "scripts": {
    "test": "nyc ava",
    "pretest": "npm-run-all lint",
    "posttest": "serverless package",
    "test:update": "ava --update-snapshots",
    "test:watch": "ava --watch",
    "test:inspect": "node --inspect node_modules/ava/profile",
    "test:inspect:watch": "nodemon --inspect node_modules/ava/profile",
    "lint": "standard",
    "clean": "del dist",
    "build": "npm-run-all --parallel build:*",
    "prebuild": "npm-run-all clean",
    "build:index": "babel --source-maps --out-dir dist index.js",
    "build:lib": "babel --source-maps --ignore='**/*.spec.js' --out-dir dist/lib lib",
    "build:handlers": "babel --source-maps --ignore='**/*.spec.js' --out-dir dist/handlers handlers",
    "dist": "npm-run-all build",
    "predist": "npm-run-all test",
    "preversion": "npm-run-all dist",
    "postversion": "git push && git push --tags",
    "deploy": "serverless deploy --aws-s3-accelerate",
    "release:staging": ".drone/deploy.sh staging",
    "release:production": ".drone/deploy.sh production",
    "example": "babel-node examples",
    "example:watch": "nodemon --exec babel-node examples",
    "example:inspect": "babel-node --inspect examples",
    "example:inspect:watch": "nodemon --exec babel-node --inspect examples",
    "offline": "serverless offline",
    "pretty": "prettier --ignore-path .gitignore --write '**/*.(js|json|graphql)'",
    "format": "npm-run-all --serial pretty format:*",
    "format:js": "standard --fix",
    "report": "nyc report"
  },
  "babel": {
    "plugins": [
      "source-map-support"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "ava": {
    "files": [
      "**/*.spec.js",
      "!dist/**/*",
      "!package/**/*"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "nyc": {
    "exclude": [
      "**/index.js",
      "dist/**/*.js",
      "package/**/*.js",
      "examples/**/*.js",
      "**/*.spec.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.spec.js",
      "test/",
      "tmp/"
    ]
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "resolutions": {
    "aws-sdk": "2.631.0"
  },
  "dependencies": {
    "aws-sdk": "2.631.0",
    "source-map-support": "^0.5.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@meltwater/examplr": "^4.2.0",
    "ava": "^3.3.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-source-map-support": "^2.0.1",
    "del-cli": "^3.0.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "serverless": "^1.58.0",
    "serverless-offline": "^6.1.2",
    "serverless-webpack": "^5.2.0",
    "standard": "^14.3.1",
    "webpack": "^4.20.2",
    "webpack-node-externals": "^2.5.0"
  }
}
